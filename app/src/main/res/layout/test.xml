<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layoutDirection="rtl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transBlack"
    tools:context="com.max.gathernclient.gathernclient.Test">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@color/mywhite"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <com.max.gathernclient.gathernclient.MyTextView
                android:id="@+id/cancel"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textAlignment="gravity"
                android:padding="8dp"
                android:gravity="end"
                android:textColor="@color/colorPrimary"
                android:text="cancel"
                />
            <com.max.gathernclient.gathernclient.MyTextView
                android:id="@+id/done"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
               android:padding="8dp"
                android:textColor="@color/colorPrimary"
                android:text="done"
                />
        </LinearLayout>
<View
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:background="@color/mydarkgray"
    />
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center_horizontal"
        >

        <com.max.gathernclient.gathernclient.MyScrollView
            android:id="@+id/dayScrollView"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:scrollbars="none"
            >
            <LinearLayout
                android:id="@+id/dayLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

            </LinearLayout>
        </com.max.gathernclient.gathernclient.MyScrollView>
        <com.max.gathernclient.gathernclient.MyScrollView
            android:id="@+id/mScrollView"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:scrollbars="none"
            >
            <LinearLayout
                android:id="@+id/monthLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <View
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:id="@+id/firstText"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="01"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="02"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="03"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="04"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="05"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="06"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="07"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="08"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="09"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="10"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:text="11"
                    />
                <com.max.gathernclient.gathernclient.MyTextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAlignment="gravity"
                    android:gravity="center"
                    android:textColor="@color/myblack"
                    android:textSize="20sp"
                    android:text="12"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    />

            </LinearLayout>
        </com.max.gathernclient.gathernclient.MyScrollView>
        <com.max.gathernclient.gathernclient.MyScrollView
            android:id="@+id/yearScrollView"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:scrollbars="none"

            >
            <LinearLayout
                android:id="@+id/yearLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
            </LinearLayout>
        </com.max.gathernclient.gathernclient.MyScrollView>

    </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#80FFFFFF"
                android:clickable="false"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/mydarkgray"
                android:layout_marginTop="110dp"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_gravity="bottom"
                android:background="@color/mydarkgray"
                android:layout_marginBottom="110dp"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#80FFFFFF"
                android:clickable="false"
                android:layout_gravity="bottom"
                />
        </FrameLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
